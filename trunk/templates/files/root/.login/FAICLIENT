#if $?LOGINDONE exit 0
#setenv LOGINDONE

setenv PRINTER kyocera

setenv MYPATH	$HOME/bin:/bin:/usr/X11R6/bin:/sbin
setenv MYMAN	''
setenv MYLIB	''

# DO NOT EDIT FOLLOWING LINES UNTIL EOD
# Your environ is : usrlocal gnu X11R5 solaris sbin tex vollocal  
setenv STDPATH /usr/local/bin:/vol/gnu/bin:/usr/bin:/usr/openwin/bin:/usr/ccs/bin:/usr/sbin:/vol/tex/bin:/vol/local/bin:/opt/SUNWspro/bin:/usr/dt/bin:/usr/proc/bin
setenv LD_LIBRARY_PATH ${MYLIB}:/usr/local/lib:/vol/gnu/lib:/usr/openwin/lib:/usr/dt/lib:/vol/X11R6/lib:/vol/local/lib:/opt/SUNWspro/lib
setenv MANPATH ${MYMAN}:/usr/local/man:/vol/gnu/man:/vol/X11R5/man:/usr/man:/usr/openwin/man:/usr/dt/man:/vol/tex/man:/vol/local/man:/opt/SUNWspro/man
# EOD # DO NOT EDIT UNTIL THIS LINE

setenv EDITOR emacs
setenv PAGER less
setenv LESS QMci
setenv LESSOPEN "|lesspipe %s"

setenv  NFSROOT         /usr/lib/fai/nfsroot
setenv  OPENWINHOME	/usr/openwin
setenv  TMPDIR		/tmp
setenv	TEXINPUTS	.:${HOME}/tex/styles:
setenv  BIBINPUTS       /vol/informatik/bib:/vol/informatik/bib/papers

setenv	PATH ${MYPATH}:${STDPATH}:.
unsetenv STDPATH MYPATH MYMAN MYLIB

if ( $?DISPLAY == 0 && $?REMOTEHOST == 1 ) then
   setenv DISPLAY ${REMOTEHOST}:0.0
endif
if ( $?USER == 0 || $?term == 0 ) exit 0
set ttyoutput = `tty`
if ($ttyoutput != "/dev/console" || $ttyoutput == 'not a tty' ) exit 0
unset ttyoutput
source $HOME/.choose_windows
