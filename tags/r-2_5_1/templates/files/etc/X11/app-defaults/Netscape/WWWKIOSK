*editorDrawingArea.translations: #replace \
	<Btn3Down>:  xfeDoClick(single,{,select,grab,})\n\
	Shift<Btn1Down>:  xfeDoClick(single,{,select,grab,})\n\
	<Btn1Down>:  xfeDoClick(single,{,select,grab,})\n\
!			     double,{,objectIs,text,  {,select,word,}, \
!					       image, {,dialog,image,}, \
!					       hrule, {,dialog,hrule,}, \
!					       target,{,dialog,target,}, \
!					       tag,   {,dialog,tag,},})\n\
	<Btn1Motion>:   xfeDoClick(single,{,select,grab,})\n\
	<Btn1Up>:	xfeDoClick(single,{,select,grab,})\n\
	<Btn2Down>:	xfeDoClick(single,{,select,grab,})\n\
	<Btn2Motion>:	xfeDoClick(single,{,select,grab,})\n\
	<Btn2Up>:	xfeDoClick(single,{,select,grab,})\n\
	<Motion>:     editor-motion()\n
!
!	 <Btn3Down>:  xfeDoCommand(showPopup)\n\
!	 Shift<Btn1Down>:  xfeDoCommand(select,extend)\n\
!	 <Btn1Down>:  xfeDoClick(single,{,select,grab,}, \
!			     double,{,objectIs,text,  {,select,word,}, \
!					       image, {,dialog,image,}, \
!					       hrule, {,dialog,hrule,}, \
!					       target,{,dialog,target,}, \
!					       tag,   {,dialog,tag,},})\n\
!	 <Btn1Motion>:			xfeDoCommand(select,extend)\n\
!	 <Btn1Up>:				xfeDoCommand(select,end)		\n\
!	 <Btn2Down>:				xfeDoCommand(paste,selection)\n\
!	 <Btn2Motion>:			\n\
!	 <Btn2Up>:				\n\
!	 <Motion>:     editor-motion()\n
!
! Editor keyboard translations.
*editorTranslations:			#override			\n\
									\
