# /etc/nsswitch.conf
#
# Example configuration of GNU Name Service Switch functionality.
# Information about this file is available in the `libc6-doc' package.

passwd:         files nis
group:          files nis
shadow:         compat

hosts:          files nis dns
networks:       files

protocols:      db files nis
services:       db files nis
ethers:         db files nis 
rpc:            db files nis

netgroup:       files nis
